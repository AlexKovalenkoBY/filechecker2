	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"reAttach": true,
			"name": "Launch filer.html",
			"file": "${workspaceFolder}/filer.html"
		},
		{
			"name": "Launch Chrome",
			"request": "launch",
			"type": "pwa-chrome",
			"url": "./filer.html",
			"webRoot": "${workspaceFolder}"
		},


		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "y:\\ARIS\\ArisApi\\uploadFiles.js"
		}
	]
}